{
  "name": "sockr-server",
  "version": "1.1.0",
  "description": "Server-side websocket messaging implementation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-180Memes/sockr.git",
    "directory": "packages/server"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "messaging",
    "server",
    "real-time",
    "typescript"
  ],
  "author": "Adeoluwa Agbakosi <adeoluwaagbakosi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "express": "^5.2.1",
    "socket.io": "^4.8.3",
    "sockr-shared": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}