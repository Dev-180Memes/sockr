{
  "name": "sockr-client",
  "version": "1.0.0",
  "description": "Client-side websocket messaging with react hooks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-180Memes/sockr.git",
    "directory": "packages/client"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "messaging",
    "client",
    "react",
    "hooks",
    "real-time",
    "typescript"
  ],
  "author": "Adeoluwa Agbakosi <adeoluwaagbakosi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "socket.io-client": "^4.8.3",
    "sockr-shared": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "react": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}